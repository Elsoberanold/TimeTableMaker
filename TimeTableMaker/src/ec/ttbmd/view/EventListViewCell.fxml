<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane fx:id="rootPane" maxHeight="88.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="88.0" prefWidth="600.0" stylesheets="@MaterialDesign.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <HBox maxHeight="88.0" prefWidth="200.0" spacing="12.0">
         <children>
            <ToggleButton fx:id="tickButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" styleClass="round-tick" />
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="2.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="headLabel" styleClass="head-label" text="Header label" />
                  <Label fx:id="subHeadLabel" styleClass="sub-head-label" text="Sub-header label" />
                  <Label fx:id="normalLabel" styleClass="normal-label" text="Normal label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </padding>
      </HBox>
      <HBox alignment="TOP_RIGHT" maxHeight="88.0" prefWidth="200.0" GridPane.columnIndex="1">
         <children>
            <Label fx:id="roomLabel" styleClass="room-label" text="Room label" />
         </children>
         <padding>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </padding>
      </HBox>
   </children>
</GridPane>
